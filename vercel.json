{
  "version": 2,
  "builds": [
    { "src": "/server/*", "use": "@vercel/node" },
    { "src": "/client/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/client/(.*)", "dest": "/client/index.html" },
    { "src": "/server/(.*)", "dest": "/server/index.ts" },
    { "src": "/(.*)", "dest": "/client/index.html" }
  ]
}