{
  "version": 2,
  "builds": [
    { "src": "/server/*", "use": "@vercel/node" },
    { "src": "/client/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem", "src": "/server/(.*)" },
    { "handle": "filesystem", "src": "/client/(.*)" },
    { "src": "/(.*)", "dest": "/client/index.html" }
  ]
}